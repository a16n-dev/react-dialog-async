  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^9.36.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "tslib": "^2.8.1",
    "typescript": "~5.8"
  }
}
